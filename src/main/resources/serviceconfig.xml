<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="emailService" class="com.springcore.setterinjection.EmailService"/>
    <bean name="smsService" class="com.springcore.setterinjection.MsgService"/>
    <bean name="reportService" class="com.springcore.setterinjection.ReportService" p:messegeService-ref="emailService"/>
</beans>